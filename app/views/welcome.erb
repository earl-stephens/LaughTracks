<head>
  <title>Comedians</title>
  </head>
  <body>
    <header>
    <h1>Welcome to Comedians!!</h1>
    </header>
    <nav>
      <img src="hqdefault.jpg" alt="laughing monkey emoji" />
    </nav>
    <div>
      <% @comedians.each do |comedian| %>
    <ul>
      <li>Name: <%= comedian.name %></li>
      <li>Age: <%= comedian.age %></li>
      <li>City: <%= comedian.city %></li>
      <% @specials.each do |special| %>
      <% if special.comedian_id == comedian.id %>
      <li><%= special.special_name %></li>
      <% end %>
      <% end %>
    </ul>
    <% end %>
    </div>

  </body>
